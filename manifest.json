{
    "name": "Rice Serveries",
    "version": "1.0.0.8",
    "description": "Make it easier to see what's for lunch! Replace New Tab page with an easy-to-read dashboard of Rice University's servery menus.",
    "manifest_version": 2,
    "chrome_url_overrides": {
        "newtab":"new-new-tab.html"
    },
    "icons": { "16": "Icons/icon16.png",
        "32":  "Icons/icon48.png",
       "128": "Icons/icon128.png" },
    "browser_action": {
	},
    "permissions": [
        "http://dining.rice.edu/*",
        "storage"
    ],
    "background": { 
        "scripts": [ "background.js" ],
        "persistent": false
    }
}